let num = [5, 1, 6, 8, 7, 9, 4, 3]
num.sort() // Organiza de forma crescente.
num.push(10) // adiciona elemento ao final do vetor.
num.pop() // Remove o Ultimo elemento do vetor.
num.shift() // Remove o primeiro elemento.
console.log(num)
console.log(`O vetor tem ${num.length} posições`) // mostra a quantidade de elementos no vetor.
console.log(`O primeiro valor do vetor é ${num[0]}`)